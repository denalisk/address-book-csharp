@{
    Layout="Shared/_Layout.cshtml";
}

<form id="new-contact" action="new" method="post">
    <div class="form-group">
        <label for="new-name">Name</label>
        <input type="text" id="new-name" name="new-name" class="form-control" value="JOE SHMOE">
    </div>
    <div class="form-group">
        <label for="new-phone-number">Phone Number</label>
        <input type="text" id="new-phone-number" name="new-phone-number" class="form-control" value="9">
    </div>
    <div class="form-group">
        <label for="new-email">Email</label>
        <input type="text" id="new-email" name="new-email" class="form-control" value="EMAIL@SITE.COM">
    </div>
    <div class="form-group">
        <label for="new-street-address">Street Address</label>
        <input type="text" id="new-street-address" name="new-street-address" class="form-control" value="6969 SOME STREET">
    </div>
    <div class="form-group">
        <label for="new-city">City</label>
        <input type="text" id="new-city" name="new-city" class="form-control" value="METROPOCITY">
    </div>
    <div class="form-group">
        <label for="new-state">State</label>
        <input type="text" id="new-state" name="new-state" class="form-control" value="STATE">
    </div>
    <button type="submit" class="btn btn-primary">Add Contact</button>
</form>
