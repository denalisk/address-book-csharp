@{
    Layout="Shared/_Layout.cshtml";
}

<form id="new-contact" action="new">
    <div class="form-group">
        <label for="new-name">First Name</label>
        <input type="text" id="new-name" name=id="new-name" class="form-control">
    </div>
    <div class="form-group">
        <label for="new-phone-number">Phone Number</label>
        <input type="number" id="new-phone-number" name="new-phone-number" class="form-control">
    </div>
    <div class="form-group">
        <label for="new-email">Email</label>
        <input type="text" id="new-email" name="new-email" class="form-control">
    </div>
    <div class="form-group">
        <label for="new-street">Street Address</label>
        <input type="text" id="new-street" name="new-street" class="form-control">
    </div>
    <div class="form-group">
        <label for="new-city">City</label>
        <input type="text" id="new-city" name="new-city" class="form-control">
    </div>
    <div class="form-group">
        <label for="new-state">State</label>
        <input type="text" id="new-state" name="new-state" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary">Add Contact</button>
</form>
